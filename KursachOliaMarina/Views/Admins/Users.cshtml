@model KursachOliaMarina.Models.CanteenContext
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";

}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Users.First().LName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.First().FName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.First().Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.First().Sex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.First().Visit)
        </th>
    </tr>

    @foreach (var item in Model.Users)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sex)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Visit)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
